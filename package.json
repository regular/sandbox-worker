{
  "name": "sandbox-worker",
  "version": "1.0.0",
  "description": "securely run untrusted JS functions in an isolated quickjs runtime inside a worker thread",
  "main": "index.js",
  "scripts": {
    "test": "tape test/**",
    "prepare": "browserify -u debug -u worker_threads --node thread.js > _thread_bundled.js"
  },
  "keywords": [
    "sandbox",
    "quickjs",
    "isolate"
  ],
  "author": "Jan BÃ¶lsche <jan@lagomorph.de> (https://regular.codes/)",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.1.1",
    "quickjs-emscripten": "^0.2.0"
  },
  "devDependencies": {
    "browserify": "^16.5.1",
    "tape": "^5.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regular/sandbox-worker.git"
  },
  "bugs": {
    "url": "https://github.com/regular/sandbox-worker/issues"
  },
  "homepage": "https://github.com/regular/sandbox-worker#readme"
